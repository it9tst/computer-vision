<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  GLOBAL  -->
    <SolidColorBrush x:Key="DefaultFontColor" Color="#FF585858" />
    <SolidColorBrush x:Key="WindowBG" Color="#FFDDDDDD" />

    <!--  GROUPBOX  -->
    <LinearGradientBrush x:Key="GroupBoxBorder" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="#AAAAAA" />
        <GradientStop Offset="0.10" Color="#c8c8c8" />
        <GradientStop Offset="0.90" Color="#c8c8c8" />
        <GradientStop Offset="1" Color="#efefef" />
    </LinearGradientBrush>
    <SolidColorBrush x:Key="GroupBoxBG" Color="#FFD7D7D7" />

