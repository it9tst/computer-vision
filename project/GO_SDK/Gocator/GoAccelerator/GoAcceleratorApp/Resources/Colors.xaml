        <LinearGradientBrush.GradientStops>
            <GradientStop Offset="0" Color="#FFFCFCFC" />
            <GradientStop Offset="0.241379" Color="#FFC3C3C3" />
            <GradientStop Offset="1" Color="#FF6B6B6B" />
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="CheckBoxBGPressed" StartPoint="0.500001,0.999985" EndPoint="0.500001,-1.4676e-005">
