        <GradientStop Offset="0.841" Color="#FFEAEAEA" />
        <GradientStop Offset="1" Color="#FF8D8D8D" />
    </LinearGradientBrush>

    <!--  IntegerUpDown  -->
    <LinearGradientBrush x:Key="IntegerUpDownBorder" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0.0" Color="#FF888888" />
        <GradientStop Offset="0.03" Color="#FF888888" />
        <GradientStop Offset="0.06" Color="#FFAAAAAA" />
        <GradientStop Offset="0.93" Color="#FFAAAAAA" />
        <GradientStop Offset="0.95" Color="#FFDDDDDD" />
        <GradientStop Offset="0.97" Color="White" />
		<GradientStop Offset="1" Color="White" />
