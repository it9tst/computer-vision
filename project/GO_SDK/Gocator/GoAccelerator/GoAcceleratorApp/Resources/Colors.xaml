    <!--  BUTTON  -->
    <!--  default  -->
    <LinearGradientBrush x:Key="ButtonBorder" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0.0" Color="#FFFFFFFF" />
        <GradientStop Offset="0.841" Color="#FFEAEAEA" />
        <GradientStop Offset="1" Color="#FF8D8D8D" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ButtonBG" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0.0" Color="#FFFFFFFF" />
        <GradientStop Offset="1.0" Color="#FFE1E1E1" />
    </LinearGradientBrush>
    <!--  hover  -->
    <LinearGradientBrush x:Key="ButtonBorderHover" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0.0" Color="#FFFFFFFF" />
        <GradientStop Offset="0.841" Color="#FFEAEAEA" />
        <GradientStop Offset="1" Color="#FF8D8D8D" />
    </LinearGradientBrush>
    <SolidColorBrush x:Key="ButtonBGHover" Color="White" />
    <!--  pressed  -->
    <LinearGradientBrush x:Key="ButtonBGPressed" StartPoint="0,0" EndPoint="0,1">
