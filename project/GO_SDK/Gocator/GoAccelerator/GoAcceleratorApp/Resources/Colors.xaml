    <SolidColorBrush x:Key="CheckBoxForeground" Color="#FF585858" />
    <!--  default  -->
    <LinearGradientBrush x:Key="CheckBoxBorder" StartPoint="0.500001,0.974286" EndPoint="0.500001,0.0256866">
        <LinearGradientBrush.GradientStops>
            <GradientStop Offset="0" Color="#FFFCFCFC" />
            <GradientStop Offset="0.241379" Color="#FF979797" />
            <GradientStop Offset="1" Color="#FF575757" />
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="CheckBoxBG" StartPoint="0.500001,0.999985" EndPoint="0.500001,-1.4676e-005">
        <LinearGradientBrush.GradientStops>
            <GradientStop Offset="0" Color="#FFA8A8A8" />
            <GradientStop Offset="1" Color="#FF575757" />
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
    <!--  hover  -->
    <LinearGradientBrush x:Key="CheckBoxBorderHover" StartPoint="0.500001,0.974286" EndPoint="0.500001,0.0256866">
